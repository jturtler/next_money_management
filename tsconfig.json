{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "sourceMap": false,
    "jsx": "preserve",
    "incremental": true,
    "strictNullChecks": true,
    "typeRoots": [
      "./types",
      "./node_modules/@types",
      "app/libs"
    ],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./app/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "app",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
],
  "include_BK": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/lib/types/**/*.d.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "lib": [
    "es6"
  ]
}
